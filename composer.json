{
    "name": "signifly/nova-order-field",
    "description": "A Laravel Nova Order field based on spatie/eloquent-sortable.",
    "keywords": [
        "laravel",
        "nova",
        "ordering"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "spatie/eloquent-sortable": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Signifly\\Nova\\Fields\\Order\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Signifly\\Nova\\Fields\\Order\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}